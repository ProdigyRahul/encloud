{
  "name": "env-vault",
  "displayName": "env_vault",
  "description": "keep track of your env files, in your local env. despite switching os",
  "version": "0.1.1",
  "publisher": "AumTamboli",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "authentication": [
      {
        "id": "supabase-github",
        "label": "Supabase (GitHub)"
      }
    ],
    "commands": [
      {
        "command": "supabase-github.logout",
        "title": "Supabase: Logout"
      },
      {
        "command": "envsync.login",
        "title": "Login to Supabase"
      },
      {
        "command": "envsync.sync",
        "title": "Sync .env Files"
      },
      {
        "command": "envsync.restore",
        "title": "Restore .env Files"
      },
      {
        "command": "envsync.clear",
        "title": "Clear .env Data"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/berzz26/env-vault"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "dotenv": "^16.5.0",
    "open": "^10.1.2"
  }
}
